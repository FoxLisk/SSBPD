@{
    ViewBag.Title = "UploadImages";
}

<h2>UploadImages</h2>

<form name="uploadImage" action="/uploadImage" enctype="multipart/form-data" method="post">
    <div>
        <label for="file">File:</label>
        <input type="file" name="file" />
    </div>
    <div>
        <input type="submit" value="upload" />
    </div>
</form>